{
  "name": "climate-microsite",
  "description": "Climate Microsite",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper export --legacy",
    "export": "sapper export --legacy",
    "now": "sapper export --legacy",
    "start": "sapper dev",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "sync": "node sync-data.js",
    "deploy": "now -e MAPS_API_KEY=@maps_api_key -e MAPS_API_SECRET=@maps_api_secret --scope=okito --target=production"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "dotenv": "^8.1.0",
    "focus-visible": "^5.0.2",
    "latlon-formatter": "^0.3.0",
    "ndarray": "*",
    "polka": "next",
    "regl": "*",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "airtable": "^0.7.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
